---
import { Icon } from "astro-icon/components";
---

<template id="group-icons">
  <Icon name="plus" id="group-open" class="h-6 w-6" />
  <Icon name="minus" id="group-close" class="h-6 w-6" />
</template>

<template id="nav-controller-icons">
  <Icon name="menu" class="mx-auto h-12 w-12" id="nav-controller-open" />
  <Icon name="x" class="mx-auto h-12 w-12" id="nav-controller-close" />
</template>

<template id="nav-folder-element">
  <nav-folder></nav-folder>
</template>

<template id="theme-icons">
  <Icon name="sun" id="theme-swap-dark" class="m-1.5 h-6 w-6" />
  <Icon name="moon" id="theme-swap-light" class="m-1.5 h-6 w-6" />
</template>
<Icon name="link" class="hidden" />
