---
import type { BoldCommentModel, BuildContext, PageModel } from "docs-model";
import Comments from "../Comments.astro";
interface Props {
    comment: BoldCommentModel;
    context: BuildContext<PageModel>;
    flatten?: boolean;
}
const { comment, context } = Astro.props;
---

<span class="font-bold"><Comments context={context} comments={comment.content} flatten={true}/></span>
