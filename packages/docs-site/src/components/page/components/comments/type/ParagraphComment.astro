---
import type {
    BuildContext,
    PageModel,
    ParagraphCommentModel,
} from "docs-model";
import Comments from "../Comments.astro";
interface Props {
    comment: ParagraphCommentModel;
    context: BuildContext<PageModel>;
    flatten?: boolean;
}
const { comment, context } = Astro.props;
---

<p>
    <Comments context={context} comments={comment.content} />
</p>
