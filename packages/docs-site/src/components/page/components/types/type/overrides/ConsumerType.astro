---
import {
    type PageModel,
    type BuildContext,
    type JavaTypeModel,
    TypeModel,
} from "docs-model";
import Type from "../../Type.astro";
interface Props {
    type: JavaTypeModel;
    context: BuildContext<PageModel>;
}
const { type, context } = Astro.props;

const tParam = Object.keys(type.typeParameters)[0];
const param = type.typeParameters[tParam];
const isObject = TypeModel.isObject(param.key);
---

<span>function({tParam.toLowerCase()} as {isObject ? tParam : <Type type={param} context={context} location="parameter" />}) as void</span>
