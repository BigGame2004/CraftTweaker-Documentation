{
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": "./src",
    "esModuleInterop": true,
    "lib": [
      "esnext"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "strict": true,
    "target": "ESNext",
    "paths": {
      "docs-model": [
        "../../docs-model/src"
      ],
      "remark-search": [
        "../../remark-search/src"
      ]
    }
  }
}
