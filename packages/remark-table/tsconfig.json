{
    "compilerOptions": {
        "allowJs": false,
        "baseUrl": ".",
        "esModuleInterop": true,
        "lib": [
            "esnext"
        ],
        "module": "ESNext",
        "moduleResolution": "node",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitAny": true,
        "outDir": "dist",
        "resolveJsonModule": true,
        "strict": true,
        "target": "ESNext"
    }
}
